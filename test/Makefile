# Makefile
#
# Generated by phxrpc_pb2server from magna.proto
#
#

include /usr/local/etc/phxrpc/phxrpc.mk

LDFLAGS := -L$(PHXRPC_ROOT)/lib -lphxrpc $(LDFLAGS) -lrt -lpthread
CFLAGS := /usr/src/linux-headers-4.4.0-87/include

# choose to use boost for network
#LDFLAGS := $(PLUGIN_BOOST_LDFLAGS) $(LDFLAGS)

SVR_OBJS = magna_main.o

CLI_OBJS = magna.pb.o \
		magna_client.o \
		phxrpc_magna_stub.o

TARGETS = magna_main

all: $(TARGETS)

magna_main: $(SVR_OBJS)
	$(LINKER) $^ $(LDFLAGS) -o $@

libmagna_client.a: $(CLI_OBJS)
	$(AR) $@ $^

magna_tool_main: phxrpc_magna_tool.o magna_tool_impl.o magna_tool_main.o
	$(LINKER) $^ -L. -lmagna_client $(LDFLAGS) -o $@

########## message ##########

magna.pb.cc: magna.pb.h

magna.pb.h: magna.proto
	$(PROTOBUF_ROOT)/bin/protoc -I$(PROTOBUF_ROOT)/include --cpp_out=. -I$(PHXRPC_ROOT) -I. $^

########## client ##########

phxrpc_magna_stub.cpp: phxrpc_magna_stub.h
phxrpc_magna_stub.o: phxrpc_magna_stub.h
magna_client.cpp: phxrpc_magna_stub.h
magna_client.o: phxrpc_magna_stub.h

phxrpc_magna_stub.h: magna.proto
	$(PHXRPC_ROOT)/codegen/phxrpc_pb2client $(PBFLAGS) -f $^ -d . -p mqtt

########## service ##########

phxrpc_magna_service.cpp: phxrpc_magna_service.h
phxrpc_magna_service.o: phxrpc_magna_service.h
magna_service_impl.cpp: phxrpc_magna_service.h
magna_service_impl.o: phxrpc_magna_service.h
phxrpc_magna_dispatcher.cpp: phxrpc_magna_service.h
phxrpc_magna_dispatcher.o: phxrpc_magna_service.h

phxrpc_magna_service.h: magna.proto
	$(PHXRPC_ROOT)/codegen/phxrpc_pb2service $(PBFLAGS) -f $^ -d . -p mqtt

########## tool ##########

phxrpc_magna_tool.cpp: phxrpc_magna_tool.h
phxrpc_magna_tool.o: phxrpc_magna_tool.h
magna_tool_impl.cpp: phxrpc_magna_tool.h
magna_tool_impl.o: phxrpc_magna_tool.h
magna_tool_main.cpp: phxrpc_magna_tool.h
magna_tool_main.o: phxrpc_magna_tool.h

phxrpc_magna_tool.h: magna.proto
	$(PHXRPC_ROOT)/codegen/phxrpc_pb2tool $(PBFLAGS) -f $^ -d . -p mqtt

clean:
	@($(RM) $(TARGETS))
	@($(RM) *.o)
	@($(RM) phxrpc_*)
	@($(RM) *.pb.*)

