include /usr/local/etc/phxrpc/phxrpc.mk

LDFLAGS := -L$(PHXRPC_ROOT)/lib -lphxrpc $(LDFLAGS)

# choose to use boost for network
#LDFLAGS := $(PLUGIN_BOOST_LDFLAGS) $(LDFLAGS)

SIMU_CLIENT_OBJS = SimuMain.o 
				

TARGETS = simu_client 

all: $(TARGETS)

simu_client: $(SIMU_CLIENT_OBJS)
	$(LINKER) $^ -L../AdminServer/ -ladmin_client -L../Comp/ -lcomp_client $(LDFLAGS) -o $@


SimuMain.o: SimuMain.cpp


clean:
	@($(RM) $(TARGETS))
	@($(RM) *.o)
	@($(RM) phxrpc_*)
	@($(RM) *.pb.*)

